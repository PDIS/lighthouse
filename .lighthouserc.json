{
  "ci": {
    "collect":{
      "numberOfRuns": 1,
      "url": [
        "https://moda.gov.tw",
        "https://pdis.nat.gov.tw"
      ],
      "settings": {
        "preset": "desktop"
      }
    },
    "assert": {
      "preset": "lighthouse:no-pwa",
      "assertions":{
        "categories:performance": ["warn", {"minScore": 0.9}],
        "categories:accessibility": ["error", {"minScore": 1}]
      }
    }
  }
}
